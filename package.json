{
  "name": "md-share",
  "version": "1.0.0",
  "description": "application for share Markdown files",
  "author": "donBarbos <donbarbos@proton.me>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/donBarbos/md-share"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "export": "next export",
    "start": "next start",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:js:fix": "yarn lint --fix",
    "lint:css": "stylelint src/**/*.{scss,css}",
    "lint:css:fix": "yarn lint:css --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,json} *.{js,ts,json,md} .eslintrc .prettierrc .stylelintrc",
    "prettier:check": "yarn prettier --check",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "yarn lint:fix"
    ],
    "*.scss": [
      "prettier --config ./.prettierrc --write",
      "stylelint --fix"
    ],
    "{*.{json,md}}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "dependencies": {
    "@next/font": "^13.1.1",
    "@svgr/webpack": "^6.5.1",
    "focus-visible": "^5.2.0",
    "markdown-it": "^13.0.1",
    "next": "^13.1.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "18.11.15",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^10.2.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-svgo": "^5.1.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "stylelint": "^14.16.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.1",
    "typescript": "4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "last 3 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
