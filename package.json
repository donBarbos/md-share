{
  "name": "md-share",
  "version": "0.0.0",
  "description": "application for share Markdown files",
  "author": "donBarbos <donbarbos@proton.me>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/donBarbos/md-share"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,json} *.{js,ts,json,md}",
    "prettier:check": "yarn prettier --check",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["prettier --config ./.prettierrc --write", "yarn lint:fix"],
    "*.scss": ["prettier --config ./.prettierrc --write", "stylelint --fix"],
    "{*.{json,md}}": ["prettier --config ./.prettierrc --write"]
  },
  "dependencies": {
    "next": "^13.0.0",
    "next-seo": "^5.8.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "18.11.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.26.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "typescript": "4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
